{
    "name": "ms-libary-ui",
    "version": "1.0.0",
    "description": "An Example component library for react ",
    "main": "Fontawesome/react-fontawesome/index.js",
    "module": "Fontawesome/react-fontawesome/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "clear": "rm -rf dist/cjs",
        "prettier": "prettier --write src/",
        "build": "rm -rf dist/cjs && npm run prettier && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
        "clean-lockfiles": "rm -rf package-lock.json yarn.lock"
    },
    "author": "ManySales",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^17.0.41",
        "@types/react": "^18.0.12",
        "@types/react-dom": "^18.0.5",
        "@types/styled-components": "^5.1.25",
        "antd": "^4.21.0",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-helmet-async": "^1.3.0",
        "typescript": "^4.7.3"
    },
    "peerDependencies": {
        "antd": ">=4",
        "react": ">=16",
        "react-dom": "^>=16",
        "react-helmet-async": ">=1.3.0"
    },
    "dependencies": {
        "styled-components": "^5.3.5"
    }
}
